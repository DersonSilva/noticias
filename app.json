{
  "expo": {
    "name": "noticias",
    "slug": "noticias",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-android.png",
    "scheme": "noticias",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7A5CFA"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-android.png",
        "backgroundColor": "#7A5CFA"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.miestrela.noticias"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "GNEWS_API_KEY": "74c53a7ac3e2e772efbc4f3f723d0a2f",
      "GNEWS_BASE_URL": "https://gnews.io/api/v4",
      "router": {},
      "eas": {
        "projectId": "9a0502c4-e838-4844-ad1e-7c7be0d6183c"
      }
    }
  }
}
